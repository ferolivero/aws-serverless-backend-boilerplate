{
    "name": "consorcios-react-backend",
    "version": "1.0.0",
    "description": "Consorcios React Backend",
    "main": "app.js",
    "license": "MIT",
    "scripts": {
        "lint": "./node_modules/.bin/eslint **/*.js  --color",
        "lint:fix": "./node_modules/.bin/eslint **/*.js  --fix --color",
        "test": "NODE_ENV=test ./node_modules/.bin/nyc --reporter=lcov --reporter=text --exclude=tests  ./node_modules/mocha/bin/mocha --require dotenv/config dotenv_config_path=./env.test tests/**/*spec.js --reporter spec;exit 0",
        "coverage": "./node_modules/.bin/nyc report --statements 80 --functions 80 --lines 80 --branches 80 --check-coverage --reporter=text-summary --per-file;exit 0",
        "prettier": "./node_modules/.bin/prettier **/*.js --write",
        "precommit": "./node_modules/.bin/prettier/lint-staged"
    },
    "dependencies": {},
    "devDependencies": {
        "aws-sdk": "^2.482.0",
        "aws-sdk-mock": "^4.4.0",
        "axios-mock-adapter": "^1.16.0",
        "husky": "^4.2.3",
        "lint-staged": "^10.1.1",
        "chai": "^4.2.0",
        "dotenv": "^8.0.0",
        "eslint": "^6.0.1",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-prettier": "^3.1.2",
        "istanbul": "^0.4.5",
        "mocha": "^6.1.4",
        "mocha-junit-reporter": "^1.23.0",
        "mock-require": "^3.0.3",
        "nock": "^10.0.6",
        "npm": "file:../../layers/node/npm/nodejs/",
        "nyc": "^14.1.1",
        "prettier": "^2.0.2",
        "prettier-eslint": "^9.0.1",
        "proxyquire": "^2.1.0",
        "sinon": "^7.3.2",
        "tap": "^14.3.1"
    }
}
